{
  "arch": {
      "type": "IterNormRobertaClassifier",
      "args": {
        "n_classes": 2,
        "cls_dropout": 0.1,
        "iteration_type": "matrix_sign",
        "num_iterations": 10,
        "use_batch_whitening": true,
        "use_running_stats_train": true,
        "use_only_running_stats_eval": true,
        "whitening_affine": false,
        "remove_biases": true,
        "use_trace_loss": true,
        "trace_loss_trade_off": 0.01,
        "log_steps_eff_rank": 10
      }
    }
}