{
    "arch": {
        "type": "BOFTRobertaClassifier",
        "args": {
          "n_classes": 2,
          "cls_dropout": 0.1,
          "boft_block_size": 8,
          "boft_n_butterfly_factor": 2,
          "use_whitening": true,
          "whitening_params": {
            "iteration_type": "matrix_root",
            "num_iterations": 3,
            "use_batch_whitening": false,
            "use_running_stats_train": true,
            "use_only_running_stats_eval": true,
            "whitening_affine": false
          },
          "remove_biases": true,
          "use_trace_loss": true,
          "trace_loss_trade_off": 0.01
        }
      }
}